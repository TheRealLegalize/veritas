#!/bin/sh 

choice=$(printf " Back\n󰨇 Process Monitor\n󰋩 Randow Wallpaper" | rofi -dmenu -p "")

case "$choice" in
    " Back")
        sh ~/bin/Menu/mainMenu
        ;;
    "󰨇 Process Monitor")
       kitty -T "floatterminal" -e btop
        ;;
    "󰋩 Randow Wallpaper")
       sh ~/bin/WppScripts/random-wallpaper.sh
        ;;
    *)
esac
