#!/bin/sh 

choice=$(printf " Back\n Hyprland\n󱔓 Waybar" | rofi -dmenu -p "")

case "$choice" in
    " Back")
        sh ~/bin/Menu/mainMenu
        ;;
    " Hyprland")
        sh ~/bin/Menu/settingsMenuHyprland
        ;;
    "󱔓 Waybar")
        sh ~/bin/Menu/settingsMenuWaybar
        ;;
    *)
esac
